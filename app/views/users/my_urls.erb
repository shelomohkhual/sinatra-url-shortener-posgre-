<div class="index row">

    <div class="column">
        <h3>MY_URLS</h3>
        <hr>
        <% if @my_urls.count != 0 %>
            <%@my_urls.each_with_index do |url,index| %>
                <%=index+1%>
                <a href='/<%=url.short_url%>'>127.0.0.1:9393/<%=". 127.0.0.1:9393/#{url.short_url} |#{url.ori_url}"%></a><br>
                <p>(<%=url.freq%>) visited this link</p>
                <hr>
            <%end %>
        <% else %>
            <h4 style="text-align:center;"> Do SOMETHING!<h4>
        <%end %>

    </div>
    
</div>
 