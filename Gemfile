# A sample Gemfile
source "https://rubygems.org"

ruby '2.6.3'

gem 'sinatra'
gem 'activerecord'
gem 'sinatra-activerecord'
gem 'rake'
gem 'require_all'
gem 'pg'
gem 'shotgun'
gem 'byebug'
gem 'bcrypt'
gem 'irb'

group :development do
gem 'dotenv'
end

group :test do
  gem 'launchy'
  gem 'rspec'
  gem 'capybara'
  gem 'rack-test'
  gem 'database_cleaner', git: 'https://github.com/bmabey/database_cleaner.git'
end
